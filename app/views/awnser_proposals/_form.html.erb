<div class="form">
	<%= form_with model: [@candidate, @proposal, @awnser_proposal], local: true do |f| %>
		<div class="field">
			<%= f.label :awnser_message %>
			<%= f.text_field :awnser_message %>
		</div>
		
		<div class="selectors">
			<div class="field">
				<%= f.label :choice_id %>
				<%= f.collection_select :choice_id, @choices, :id, :option %>
			</div>
		</div>

		<button class="button-enviar-form" type="submit">Enviar Feedback</button>
	<% end %>
</div>
