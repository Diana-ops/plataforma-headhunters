<div class="form">
	<%= form_with model: [@job_opportunuty, @apply_job, @feedback], local: true do |f| %>
		<div class="field">
			<%= f.label :message %>
			<%= f.text_field :message %>
		</div>
		
		<div class="selectors">
			<div class="field">
				<%= f.label :choice_id %>
				<%= f.collection_select :choice_id, @choices, :id, :option %>
			</div>
		</div>

		<button class="button-enviar-form" type="submit">Enviar Feedback</button>
	<% end %>
</div>
